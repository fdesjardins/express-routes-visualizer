{
  "name": "express-routes-visualizer",
  "description": "Visualize Express routes using D3",
  "version": "0.2.4",
  "author": {
    "name": "Forrest Desjardins",
    "email": "desjardinsfg@gmail.com",
    "url": "github.com/fdesjardins"
  },
  "dependencies": {},
  "devDependencies": {
    "chai": "^4.2.0",
    "coveralls": "^3.0.3",
    "mocha": "^6.0.1",
    "nyc": "^13.3.0",
    "standard": "^12.0.1"
  },
  "engines": {
    "node": ">=7"
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ],
    "rules": {
      "object-curly-spacing": [
        2,
        "always"
      ],
      "array-bracket-spacing": [
        2,
        "always"
      ],
      "func-style": [
        2,
        "expression"
      ],
      "prefer-arrow-callback": [
        2,
        {
          "allowNamedFunctions": false
        }
      ]
    }
  },
  "keywords": [
    "express",
    "express.js",
    "graph",
    "hierarchy",
    "list",
    "middleware",
    "node",
    "routes",
    "routing",
    "structure",
    "viewer",
    "visualization",
    "visualizer"
  ],
  "license": "MIT",
  "main": "index.js",
  "nyc": {
    "reporter": [
      "lcov",
      "text"
    ]
  },
  "repository": "fdesjardins/express-routes-visualizer",
  "scripts": {
    "test": "standard *.js && nyc mocha test.js"
  }
}
